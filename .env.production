# Production Environment Configuration

# API Configuration
REACT_APP_API_URL=https://api.suprisesupermarket.com/v1

# Google reCAPTCHA v2 - Get from https://www.google.com/recaptcha/admin
REACT_APP_RECAPTCHA_SITE_KEY=YOUR_PRODUCTION_RECAPTCHA_SITE_KEY

# Analytics
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX  # Replace with your production GA4 Measurement ID
REACT_APP_GTM_CONTAINER_ID=GTM-XXXXXX    # Replace with your production GTM Container ID

# Feature Flags
REACT_APP_ENABLE_DEBUG_LOGS=false
REACT_APP_ENABLE_MOCK_API=false

# Performance Monitoring
REACT_APP_SENTRY_DSN=  # Optional: Add Sentry DSN for error tracking

# CDN Configuration (if using CDN for static assets)
REACT_APP_CDN_URL=https://cdn.suprisesupermarket.com
